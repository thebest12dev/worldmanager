    "properties": {
      "schemaVersion": {
        "type": "integer"
      },
      "worldmanagerMeta": {
        "type": "object",
        "properties": {
          "version": {
            "type": "string"
          },
          "branch": {
            "type": "string"
          },
          "build": {
            "type": "integer"
          },
          "updateRepo": {
            "type": "string"
          },
          "versionCheck": {
            "type": "string"
          }
        },
        "required": [
          "version",
          "branch",
          "build",
          "updateRepo",
          "versionCheck"
        ]
      },
      "worldmanagerSettings": {
        "type": "object"
      }
    },
    "required": [
      "schemaVersion",
      "worldmanagerMeta",
      "worldmanagerSettings"
    ]
  }